wide false
sideways false
status collapsed

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Nyquist plots for (
\begin_inset Formula $D>T$
\end_inset

), left and (
\begin_inset Formula $T>D$
\end_inset

), right
\begin_inset CommandInset label
LatexCommand label
name "fig:Nyquist-plots"

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Plain Layout
